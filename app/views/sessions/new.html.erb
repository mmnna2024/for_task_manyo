<h1>ログインページ</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <div>
  <%= f.label :email, (t 'sessions.new.email') %><br>
  <%= f.email_field :email %>
  </div>

  <div>
  <%= f.label :password, (t 'sessions.new.password') %><br>
  <%= f.password_field :password %>
  </div>

  <div>
  <%= f.submit "ログイン", id: "create-session" %>
  </div>
<% end %>